generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model projects {
  id          Int      @id
  title       String?
  author      String?
  cost        Int?
  votes       Int?
  projectsize String?
  description String?
  location    String?
  isAproverd  Boolean?
  city        String?
}

model users {
  id       Int      @id
  email    String?
  password String?
  city     String?
  PESEL    Int?
  name     String?
  surname  String?
  phone    Int?
  isGov    Boolean?
}
